package Link;

public class Node<T> {

    public T data;
    public Node<T> next;
    public Node<T> prev;
    public Node<T> head;
    public int size;

    public Node(T data){
        this.data = data;
        this.next = null;
        this.prev = null;
        this.head = null;
        this.size = 0;
    }

    public Node(){
        this.data = null;
        this.next = null;
    }

    public void setHead(Node head){
        this.head = head;
    }
    public Node getHead(){
        return head;
    }

    public void setPrev(Node prev){
        this.prev = prev;
    }

    public Node getPrev(){
        return prev;
    }

    public void setData(T data){
        this.data = data;
    }
    public T getData(){
        return data;
    }
    public Node getNext(){
        return next;
    }
    public void setNext(Node next){
        this.next = next;
    }
}
